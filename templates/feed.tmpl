<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <link rel="self" type="application/atom+xml" href="{{.URL}}"/>

  <title>{{.Title}}</title>
  <updated>{{.Updated}}</updated>
  <author>
    <name>newsletter2rss</name>
  </author>
  <id>{{.ID}}</id>

{{range .Entries}}
  <entry>
    <title>{{.Title}}</title>
    <id>{{.ID}}</id>
    <updated>{{.PublishDate}}</updated>
    <link href="{{.ID}}"/>
    <author>
      <name>{{.Author}}</name>
      <uri>{{.Domain}}</uri>
    </author>
    <content type="text">{{.Content}}</content>
    <summary>{{.Summary}}</summary>
    <published>{{.PublishDate}}</published>
  </entry>
{{end}}

</feed>
